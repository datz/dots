Xcursor.size: 64

! https://github.com/majutsushi/urxvt-font-size
URxvt.perl-ext: default,matcher,bell-command,clipboard,font-size

URxvt*urgentOnBell: true
URxvt*visualBell:   false
URxvt*mapAlert:     true 

URxvt*iso14755:    false
URxvt*iso14755_52: false

URxvt*keysym.Shift-Control-C: perl:clipboard:copy
URxvt*keysym.Shift-Control-V: perl:clipboard:paste

 Xft.dpi:      160 2
Xft.antialias: true
Xft.rgba:      rgb
Xft.hinting:   true
Xft.hintstyle: hintslight

URxvt.depth:                    32
URxvt.shading:                  20
URxvt.internalBorder:           3
URxvt.lineSpace:                1

URxvt.bell-command: aplay /home/ptz/src/dots/smb3_inventory_open_close.wav

URxvt.font:		       xft:UbuntuMono Nerd Font:size=11
URxvt.boldFont		   xft:UbuntuMono Nerd Font:size=11
URxvt.italicFont     xft:UbuntuMono Nerd Font:size=11
URxvt.boldItalicFont xft:UbuntuMono Nerd Font:size=11

URxvt*letterSpace: 1

URxvt.scrollStyle: rxvt
URxvt.scrollBar:   false

URxvt.matcher.button: 1
URxvt.url-launcher:   firefox

URxvt.cursorBlink:     true
URxvt.cursorColor:     #657b83
URxvt.cursorUnderline: false

URxvt.pointerBlank: true

*pointerColorBackground: #586e75
*pointerColorForeground: #93a1a1
*fading:                 20

rofi.location:          0
rofi.color-enabled:     true
!                       'background', 'border'
rofi.color-window:      argb:dc111111, argb:dc111111
rofi.theme:             ~/.cache/wal/colors-rofi-dark.rasi
rofi.separator-style:   dash
rofi.fullscreen:        false
rofi.padding:           130
rofi.hide-scrollbar:    true
rofi.line-margin:       5
rofi.fake-transparency: false
rofi.yoffset:           0
rofi.xoffset:           0
rofi.width:             1366
rofi.opacity:           90
rofi.font: SourceCodePro-Regular 12